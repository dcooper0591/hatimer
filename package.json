{
  "name": "hatimer",
  "version": "0.1.0",
  "description": "A Lovelace card for controlling Home Assistant timer entities",
  "keywords": [
    "home-assistant",
    "lovelace",
    "hacs",
    "timer",
    "custom-card"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
